})
/* Youtube video on modal close stop playing + removes autoplay. Doesnt work though :( */
$("button#stopVideo").on("click", function(){
var video = $("#utubevideo").attr("src");
$("#utubevideo").attr("src","");
$("#utubevideo").attr("src",(video.substring(0,video.length - 11)));
})
